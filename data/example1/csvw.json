        "datatype": "integer",
        "required": true
      },
      {
        "name": "date",
        "titles": {
          "fr": "Date",
          "en": "Date"
        },
        "datatype": {
          "base": "date",
          "minInclusive": "2023-01-01",
          "maxInclusive": "2023-12-31"
        },
