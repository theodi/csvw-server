    "dc:publisher": {
        "schema:name": "FinCorp Ltd.",
        "schema:url": {"@id": "http://fincorpltd.example.com" }
    },
    "dcat:keyword": ["transactions", "financial"],
    "dc:license": {"@id": "http://opendefinition.org/licenses/cc-by/"},
