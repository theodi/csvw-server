    "tableSchema": {
      "columns": [
        {
          "name": "transaction_id",
          "titles": {
            "fr": "ID de Transaction",
            "en": "Transaction ID"
          },
          "datatype": "integer",
          "propertyUrl": "schema:identifier",
          "required": true
        },
        {
          "name": "date",
          "titles": {
            "fr": "Date",
            "en": "Date"
          },
          "datatype": {
            "base": "date",
            "minInclusive": "2023-01-01",
            "maxInclusive": "2023-12-31"
          },
          "propertyUrl": "schema:date",
          "required": true
        },
        {
          "name": "payer",
          "titles": {
             "fr": "Payeur",
             "en": "Payer"
          },
          "datatype": "string",
          "propertyUrl": "http://reference.data.gov.uk/def/payment#payer",
          "valueUrl": "http://jpmc.learndata.info/company/{payer}",
          "required": true
        },
        {
          "name": "payee",
          "titles": {
            "fr": "Bénéficiaire",
            "en": "Payee"
          },
          "datatype": "string",
          "propertyUrl": "http://reference.data.gov.uk/def/payment#payee",
          "valueUrl": "http://jpmc.learndata.info/company/{payee}",
          "required": true
        },
        {
          "name": "description",
          "titles": "Description",
          "datatype": "string",
          "propertyUrl": "schema:description",
          "required": true
        },
        {
          "name": "amount",
          "titles": {
            "fr": "Montant (USD)",
            "en": "Amount (USD)"
          },
          "datatype": "decimal",
          "required": true,
          "suppressOutput": true
        },
        {
          "virtual": true,
          "propertyUrl": "schema:amount",
          "valueUrl": "http://jpmc.learndata.info/transactions/{transaction_id}#amount"
        },
        {
          "virtual": true,
          "aboutUrl": "http://jpmc.learndata.info/transactions/{transaction_id}#amount",
          "propertyUrl": "rdf:type",
          "valueUrl": "schema:MonetaryAmount"
        },
        {
          "virtual": true,
          "aboutUrl": "http://jpmc.learndata.info/transactions/{transaction_id}#amount",
          "propertyUrl": "schema:value",
          "valueUrl": "{amount}"
        },
        {
          "virtual": true,
          "aboutUrl": "http://jpmc.learndata.info/transactions/{transaction_id}#amount",
          "propertyUrl": "schema:currency",
          "valueUrl": "USD"
        },
        {
          "virtual": true,
          "aboutUrl": "http://jpmc.learndata.info/transactions/{transaction_id}",
          "propertyUrl": "rdf:type",
          "valueUrl": "http://reference.data.gov.uk/def/payment#Payment"
        }
      ],
      "primaryKey": "transaction_id",
      "aboutUrl": "http://jpmc.learndata.info/transactions/{transaction_id}"
    }
